<%- model_class = User -%>
<div class="page-header">
  <h1><%= t('helpers.titles.new', model: model_class.model_name.human) %></h1>
</div>
<%= form_for @user do |f| %>
<%= f.text_field :name, placeholder: t('activerecord.attributes.user.name') %>
<%= f.number_field :uid, placeholder: t('activerecord.attributes.user.uid')  %>
<div class="form-actions">
  <%= f.submit nil, class: 'btn btn-primary' %>
</div>
<% end %>
